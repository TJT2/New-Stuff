using System;

namespace Test
{
    class MainClass
    {
        public static void Main(string[] args)
        {
            Console.WriteLine("Escribe Dos Numeros Por Fabor");
            float number1 = float.Parse(Console.ReadLine());
            float number2 = float.Parse(Console.ReadLine());
            Console.WriteLine(number1 - number2);
        }
    }
}
